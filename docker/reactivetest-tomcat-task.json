{
	"family": "reactivetest-tomcat",
	"networkMode": "awsvpc",
	"containerDefinitions": [
		{
			"name": "reactivetest-tomcat",
			"image": "trincao/reactivetest-tomcat:0.0.1",
			"portMappings": [
				{
					"containerPort": 8080,
					"hostPort": 8080,
					"protocol": "tcp"
				}
			],
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "/ecs/reactivetest",
					"awslogs-region": "us-east-1",
					"awslogs-stream-prefix": "ecs"
				}
			},
			"essential": true
		}
	],
	"requiresCompatibilities": [
		"FARGATE"
	],
	"cpu": "256",
	"memory": "2048",
	"executionRoleArn": "ecsTaskExecutionRole"
}